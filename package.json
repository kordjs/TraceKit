{
  "name": "@kordjs/tracekit",
  "version": "1.0.0",
  "description": "TypeScript-based terminal and remote logging library with zero dependencies",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "node --test dist/test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "logging",
    "logger",
    "terminal",
    "remote",
    "typescript",
    "websocket",
    "http",
    "ansi"
  ],
  "author": "KordJS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kordjs/tracekit.git"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}