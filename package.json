{
  "name": "@kordjs/logger",
  "version": "2.1.5",
  "description": "Advanced, flexible, and type-safe logger for Node.js projects.",
  "main": "dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "pretty": "prettier --write .",
    "lint": "eslint",
    "prepare": "npm run pretty && npm run lint && npm run build",
    "release": "release-it",
    "docs": "typedoc",
    "start": "npm run build && clear && node ./tests/.node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kordjs/logger.git"
  },
  "keywords": [
    "logger",
    "kordjs"
  ],
  "author": "kordjs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/0sapphy/logger/issues"
  },
  "homepage": "https://github.com/0sapphy/logger#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "auto-changelog": "^2.5.0",
    "eslint": "^9.30.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "release-it": "^19.0.3",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.7",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@kordjs/utils": "^1.3.0",
    "typedoc-material-theme": "^1.4.0"
  }
}
