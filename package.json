  "license": "ISC",
  "bugs": {
    "url": "https://github.com/0sapphy/logger/issues"
  },
  "homepage": "https://github.com/0sapphy/logger#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "eslint": "^9.30.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@kordjs/utils": "^1.2.1"
  }
