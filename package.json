{
  "name": "@kordjs/logger",
  "version": "2.0.4",
  "description": "A Logger.",
  "main": "dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
